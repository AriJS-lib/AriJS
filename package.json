{
  "name": "Tibcord",
  "version": "0.0.1",
  "description": "An optimized NodeJS Discord library.",
  "main": "./index.js",
  "exports": {
    ".": [
      {
        "require": "./index.js",
        "import": "./esm.mjs"
      },
      "./index.js"
    ],
    "./": "./",
    "./esm": "./esm.mjs"
  },
  "typings": "./index.d.ts",
  "engines": {
    "node": ">=10.4.0"
  },
  "scripts": {
    "lint:js": "eslint -c .eslintrc.yml lib examples *.js",
    "lint:js:fix": "eslint -c .eslintrc.yml lib examples *.js --fix && echo \"\u001b[1m\u001b[32mOK\u001b[39m\u001b[22m\" || echo \"\u001b[1m\u001b[31mNot OK\u001b[39m\u001b[22m\"",
    "lint:ts": "eslint -c .eslintrc.ts.yml *.ts",
    "lint:ts:fix": "eslint -c .eslintrc.ts.yml *.ts --fix && echo \"\u001b[1m\u001b[32mOK\u001b[39m\u001b[22m\" || echo \"\u001b[1m\u001b[31mNot OK\u001b[39m\u001b[22m\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/"
  },
  "keywords": [
    "api",
    "discord",
    "library",
    "Tibcord",
    "wrapper"
  ],
  "author": "dirquel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tibcord/Tibcord/issues"
  },
  "homepage": "https://gitub.com/Tibcord/Tibcord/#readme/",
  "dependencies": {
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/node": "18.7.15",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.23.0",
    "eslint-plugin-sort-class-members": "1.15.2",
    "typescript": "4.8.2"
  },
  "optionalDependencies": {
    "opusscript": "^0.0.8",
    "tweetnacl": "^1.0.3"
  },
  "browser": {
    "@discordjs/opus": false,
    "child_process": false,
    "dgram": false,
    "dns": false,
    "fs": false,
    "tls": false,
    "tweetnacl": false,
    "ws": false
  }
}
